        emp_pob varchar2(30 char),
        emp_tel varchar2(60 char),
        emp_crecte varchar2(1 char),
        emp_vacdianat varchar2(1 char),
        primary key (emp_cod, emp_idf_cod)
    );

    create table trhu_gfe (
       gfe_cod varchar2(4 char) not null,
